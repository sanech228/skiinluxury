{
  "name": "homeex",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "Property scraper for VillaGetaways.com",
  "author": "Sane <sanech228@gmail.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "VillaGetaways",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "npm run icon && vite",
